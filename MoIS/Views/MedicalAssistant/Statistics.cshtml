                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @*Html.ActionLink("MoIS", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
                <p class="navbar-brand">MoIS</p>
                @if (Session["maID"] != null)
                {
                    if (Session["role2"] != null)
                    {
                        @Html.ActionLink("Home", "Index", "MedicalAssistant", new { area = "" }, new { @class = "navbar-brand" })
                        @Html.ActionLink("Deceased Home", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                    }
                    else
                    {
                        @Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                    }

                    @Html.ActionLink("Password Management", "ChangePassword", "MedicalAssistant", new { area = "" }, new { @class = "navbar-brand" })

                }
            </div>
            <div class="navbar-header pull-right">
                @if (Session["maID"] != null)
