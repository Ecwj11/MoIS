    <script>
        function confirm_submit_reset() {
            var confrm;
            confrm = window.confirm("Confirm reset?");

            return confrm;
        }
        function confirm_change() {
            var confrm;
            confrm = window.confirm("Confirm change user status?");

            return confrm;
        }
        if ($('#chart_6').length > 0) {
            var ctx6 = document.getElementById("chart_6").getContext("2d");
            var data6 = {
                labels: [
                    "Male",
                    "Female",
                ],
                datasets: [
                    {
                        data: [@Model.maleTotal, @Model.femaleTotal],
                        backgroundColor: [
                            "#2879ff",
                            "#01c853",
                        ],
                        hoverBackgroundColor: [
                            "#2879ff",
                            "#01c853",
                        ]
                    }]
            };

            var pieChart = new Chart(ctx6, {
                type: 'pie',
                data: data6,
                options: {
                    animation: {
                        duration: 3000
                    },
                    responsive: true,
                    maintainAspectRatio: false,
                    legend: {
                        display: false
                    },
                    tooltip: {
                        backgroundColor: 'rgba(33,33,33,1)',
                        cornerRadius: 0,
                        footerFontFamily: "'Roboto'"
                    },
                    elements: {
                        arc: {
                            borderWidth: 0
                        }
                    }
                }
            });
        }
        if ($('#morris_extra_line_chart').length > 0) {
            var data = [{
                period: 'Sun',
                male: @Model.maleSun,
                female: @Model.femaleSun,
            }, {
                period: 'Mon',
                male: @Model.maleMon,
                female: @Model.femaleMon,
            }, {
                period: 'Tue',
                male: @Model.maleTues,
                female: @Model.femaleTues,
            }, {
                period: 'Wed',
                male: @Model.maleWed,
                female: @Model.femaleWed,
            }, {
                period: 'Thu',
                male: @Model.maleThurs,
                female: @Model.femaleThurs,
            }, {
                period: 'Fri',
                male: @Model.maleFri,
                female: @Model.femaleFri,
            },
            {
                period: 'Sat',
                male: @Model.maleSat,
                female: @Model.femaleSat,
                }];
            var dataNew = [{
                period: 'Jan',
                male: @Model.maleJan,
                female: @Model.femaleJan,
            },
            {
                period: 'Feb',
                male: @Model.maleFeb,
                female: @Model.femaleFeb,
            },
            {
                period: 'March',
                male: @Model.maleMar,
                female: @Model.femaleMar,
            },
            {
                period: 'April',
                male: @Model.maleApr,
                female: @Model.femaleApr,
            },
            {
                period: 'May',
                male: @Model.maleMay,
                female: @Model.femaleMay,
            },
            {
                period: 'June',
                male: @Model.maleJun,
                female: @Model.femaleJun,
            },
            {
                period: 'July',
                male: @Model.maleJul,
                female: @Model.femaleJul,
            },
            {
                period: 'Aug',
                male: @Model.maleAug,
                female: @Model.femaleAug,
            },
            {
                period: 'Sep',
                male: @Model.maleSep,
                female: @Model.femaleSep,
            },
            {
                period: 'Oct',
                male: @Model.maleOct,
                female: @Model.femaleOct,
            },
            {
                period: 'Nov',
                male: @Model.maleNov,
                female: @Model.femaleNov,
            },
            {
                period: 'Dec',
                male: @Model.maleDec,
                female: @Model.femaleDec,
            }
            ];
            var lineChart = Morris.Line({
                element: 'morris_extra_line_chart',
                data: data,
                xkey: 'period',
                ykeys: ['male', 'female'],
                labels: ['Male', 'Female'],
                pointSize: 2,
                fillOpacity: 0,
                lineWidth: 2,
                pointStrokeColors: ['#fec107', '#e91e63'],
                behaveLikeLine: true,
                gridLineColor: '#878787',
                hideHover: 'auto',
                lineColors: ['#fec107', '#e91e63'],
                resize: true,
                redraw: true,
                gridTextColor: '#878787',
                gridTextFamily: "Roboto",
                parseTime: false
            });

        /* Switchery Init*/
        var elems = Array.prototype.slice.call(document.querySelectorAll('.js-switch'));
        $('#morris_switch').each(function () {
            new Switchery($(this)[0], $(this).data());
        });
        var swichMorris = function () {
            if ($("#morris_switch").is(":checked")) {
                lineChart.setData(data);
                lineChart.redraw();
            } else {
                lineChart.setData(dataNew);
                lineChart.redraw();
            }
        }
        swichMorris();
        $(document).on('change', '#morris_switch', function () {
            swichMorris();
        });
        }

        /*****Load function start*****/
        $(window).load(function () {
            window.setTimeout(function () {
                $.toast({
                    heading: 'Hello @Session["username"]',
                    text: 'Welcome to MoIS.',
                    position: 'top-right',
                    loaderBg: '#fec107',
                    icon: 'success',
                    hideAfter: 3500,
                    stack: 6
                });
            }, 3000);
        });
/*****Load function* end*****/
    </script>
    <script>
        $('.table').dataTable({
            stateSave: true
        });
    </script>
</body>
