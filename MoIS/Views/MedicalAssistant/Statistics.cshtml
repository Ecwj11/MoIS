                    <a href="#" class="navbar-brand">Welcome, @Session["username"]</a>
                    <a href="@Url.Action("LogOut","Login")" class="navbar-brand"><i class="fas fa-sign-out-alt"></i>Logout</a>
                }
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav"></ul>
            </div>
